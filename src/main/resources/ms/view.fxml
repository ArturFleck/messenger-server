<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="ap_main" fx:controller="ms.Controller"
            prefHeight="396" prefWidth="478"
            xmlns:fx="http://javafx.com/fxml">
    <children>
        <Button fx:id="button_send" layoutX="404" layoutY="338" text="Send"/>
        <TextField fx:id="tf_message" layoutX="30" layoutY="338" prefHeight="26" prefWidth="361"/>
        <ScrollPane fx:id="sp_main" layoutX="30" layoutY="70" prefHeight="256" prefWidth="418" fitToWidth="true">
            <content>
                <VBox fx:id="vBox_messages" prefHeight="248" prefWidth="412"/>
            </content>
        </ScrollPane>
        <Label layoutX="114" layoutY="26" text="My Messenger">
            <font>
                <Font name="Arial" size="23"></Font>
            </font>
        </Label>
    </children>
</AnchorPane>
